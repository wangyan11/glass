    function n(a,b,c){
    var d=b+"defer",e=b+"queue",g=b+"mark",h=f._data(a,d);
    h&&(c==="queue"||!f._data(a,e))&&(c==="mark"||!f._data(a,g))&&setTimeout(function(){
